<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netstandard2.0;net472</TargetFrameworks>
    <DefaultItemExcludes>$(DefaultItemExcludes);nuget\*.nupkg;nuget\*.log</DefaultItemExcludes>
    <NoWarn>1701;1702;1591</NoWarn>
    <PackageId>DHI.Services.GIS</PackageId>
    <Version>12.1.0</Version>
    <PackageReleaseNotes>Updated DHI.Services to v15.1.0 and DHI.Spatial to v2.6.2</PackageReleaseNotes>
    <Company>DHI</Company>
    <Authors>dhigroup</Authors>
    <PackageLicenseExpression>BSD-3-Clause</PackageLicenseExpression>
    <PackageProjectUrl>https://github.com/DHI/DomainServices/blob/master/README.md</PackageProjectUrl>
    <RepositoryUrl>https://github.com/DHI/DomainServices</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <Description>GIS services and analysis.</Description>
    <Product>DHI Domain Services</Product>
    <PackageTags>Feature Collections, GeoJSON, WMS</PackageTags>
    <PackageIcon>ds-icon-64.png</PackageIcon>
    <IncludeSymbols>true</IncludeSymbols>
    <SymbolPackageFormat>snupkg</SymbolPackageFormat>
    <EmbedUntrackedSources>true</EmbedUntrackedSources>
    <LangVersion>latest</LangVersion>
    <GenerateDocumentationFile>True</GenerateDocumentationFile>
	<PackageReadmeFile>readme.md</PackageReadmeFile>
  </PropertyGroup>

  <PropertyGroup Condition="'$(GITHUB_ACTIONS)' == 'true'">
    <ContinuousIntegrationBuild>true</ContinuousIntegrationBuild>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="DHI.Services" Version="15.1.0" />
    <PackageReference Include="DHI.Spatial" Version="2.6.2" />
    <PackageReference Include="DHI.Spatial.GeoJson" Version="5.0.0" />
    <PackageReference Include="FileCache" Version="3.3.0" />
    <PackageReference Include="Microsoft.SourceLink.GitHub" Version="1.1.1">
      <PrivateAssets>all</PrivateAssets>
      <IncludeAssets>runtime; build; native; contentfiles; analyzers</IncludeAssets>
    </PackageReference>
    <PackageReference Include="SkiaSharp" Version="2.88.8" />
    <PackageReference Include="System.Runtime.Caching" Version="6.0.0" />
    <PackageReference Include="System.Text.Json" Version="8.0.5" />
  </ItemGroup>

  <ItemGroup>
	  <None Include="nuget\readme.md">
		  <Pack>true</Pack>
		  <PackagePath></PackagePath>
	  </None>
    <None Include="nuget\ds-icon-64.png" Pack="true" PackagePath="" />
  </ItemGroup>

</Project>