<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netstandard2.0;net8.0</TargetFrameworks>
    <nullable>enable</nullable>
    <DefaultItemExcludes>$(DefaultItemExcludes);nuget\*.nupkg;nuget\*.log</DefaultItemExcludes>
    <NoWarn>1701;1702;1591</NoWarn>
    <PackageId>DHI.Services.Models</PackageId>
    <Version>4.1.0</Version>
    <Company>DHI</Company>
    <Authors>dhigroup</Authors>
    <PackageReleaseNotes>Fix ModelDataReader so we don't need to manually register .dll</PackageReleaseNotes>
    <PackageProjectUrl>https://github.com/DHI/DomainServices/blob/master/README.md</PackageProjectUrl>
    <RepositoryUrl>https://github.com/DHI/DomainServices</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <Description>Model simulation services.</Description>
    <Product>DHI Domain Services</Product>
    <PackageTags></PackageTags>
    <PackageIcon>ds-icon-64.png</PackageIcon>
    <IncludeSymbols>true</IncludeSymbols>
    <SymbolPackageFormat>snupkg</SymbolPackageFormat>
    <EmbedUntrackedSources>true</EmbedUntrackedSources>
    <GenerateDocumentationFile>True</GenerateDocumentationFile>
    <LangVersion>latest</LangVersion>
	<PackageReadmeFile>readme.md</PackageReadmeFile>
  </PropertyGroup>

  <PropertyGroup Condition="'$(GITHUB_ACTIONS)' == 'true'">
    <ContinuousIntegrationBuild>true</ContinuousIntegrationBuild>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="DHI.Services" Version="16.1.0" />
    <PackageReference Include="DHI.Services.TimeSeries" Version="7.1.1" />
    <PackageReference Include="Microsoft.SourceLink.GitHub" Version="1.1.1">
      <PrivateAssets>all</PrivateAssets>
      <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
    </PackageReference>
  </ItemGroup>

  <ItemGroup>
	  <None Include="nuget\readme.md">
		  <Pack>true</Pack>
		  <PackagePath></PackagePath>
	  </None>
    <None Include="nuget\ds-icon-64.png" Pack="true" PackagePath="" />
  </ItemGroup>

</Project>