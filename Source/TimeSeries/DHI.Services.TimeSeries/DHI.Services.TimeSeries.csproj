<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netstandard2.0;net472;net8.0</TargetFrameworks>
    <DefaultItemExcludes>$(DefaultItemExcludes);nuget\*.nupkg;nuget\*.log</DefaultItemExcludes>
    <NoWarn>1701;1702;1591</NoWarn>
    <PackageId>DHI.Services.TimeSeries</PackageId>
    <Version>7.1.1</Version>
    <PackageReleaseNotes>Upgrade Domain Services Packages</PackageReleaseNotes>
    <Company>DHI</Company>
    <Authors>dhigroup</Authors>
    <Owners>dhigroup</Owners>
    <PackageLicenseExpression>BSD-3-Clause</PackageLicenseExpression>
    <PackageProjectUrl>https://github.com/DHI/DomainServices/blob/master/README.md</PackageProjectUrl>
    <RepositoryUrl>https://github.com/DHI/DomainServices</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <Description>Time series services and analysis.</Description>
    <Product>DHI Domain Services</Product>
    <PackageIcon>ds-icon-64.png</PackageIcon>
    <IncludeSymbols>true</IncludeSymbols>
    <SymbolPackageFormat>snupkg</SymbolPackageFormat>
    <EmbedUntrackedSources>true</EmbedUntrackedSources>
    <GenerateDocumentationFile>True</GenerateDocumentationFile>
    <LangVersion>latest</LangVersion>
	<PackageReadmeFile>readme.md</PackageReadmeFile>
  </PropertyGroup>

  <PropertyGroup Condition="'$(GITHUB_ACTIONS)' == 'true'">
    <ContinuousIntegrationBuild>true</ContinuousIntegrationBuild>
  </PropertyGroup>


  <ItemGroup>
    <PackageReference Include="DHI.Services" Version="16.1.0" />
    <PackageReference Include="Argon.JsonPath" Version="0.5.1" />
    <PackageReference Include="Microsoft.CSharp" Version="4.7.0" />
    <PackageReference Include="Microsoft.SourceLink.GitHub" Version="1.0.0">
      <PrivateAssets>all</PrivateAssets>
      <IncludeAssets>runtime; build; native; contentfiles; analyzers</IncludeAssets>
    </PackageReference>
  </ItemGroup>

  <ItemGroup>
	  <None Include="nuget\readme.md">
		  <Pack>true</Pack>
		  <PackagePath></PackagePath>
	  </None>
    <None Include="nuget\ds-icon-64.png" Pack="true" PackagePath="" />
  </ItemGroup>

</Project>